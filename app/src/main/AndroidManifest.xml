<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.noranekoit.made.submission">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.noranekoit.made.submission.MyApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Submission1">
        <activity
            android:name="com.noranekoit.made.submission.detail.DetailMovieActivity"
            android:exported="false"
            android:label="@string/title_activity_detail_home"
            android:theme="@style/Theme.Submission1.NoActionBar" />
        <activity
            android:name="com.noranekoit.made.submission.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Submission1.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>